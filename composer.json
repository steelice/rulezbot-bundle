{
  "description": "Bots bundle",
  "name": "steelice/rulezbot-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["telegram", "bot"],
  "authors": [
    {
      "name": "Steel Ice"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "ext-intl": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.16",
    "nesbot/carbon": "^2.68",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.23",
    "steelice/telegrambotapi": "^1.0",
    "stof/doctrine-extensions-bundle": "^1.8",
    "symfony/doctrine-messenger": "^6.3",
    "symfony/http-kernel": "^6.3",
    "ext-curl": "*"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/steelice/TelegramBotApi.git"
    }
  ],
  "autoload": {
    "psr-4": { "Rulezdev\\RulezbotBundle\\": "src/" }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.50"
  }
}
